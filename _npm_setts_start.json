{
    "ignore_this": "Seguir todas estas configuraciones en oreden",
    "env": "npm install dotenv, npm install --save-dev @types/dotenv",
    "nodemon": "npm i nodemon",
    "UUID": "npm i uuid",
    "@types/node": "npm i -D typescript @types/node",
    "axios": "npm i axios",
    "winston": "npm i winston",
    "ts-node": "npm i -D ts-node nodemon",
    "npx_tsc": "npx tsc --init --outDir dist/ --rootDir src",
    "transpilator": "npx tsc --watch",
    "create_nodemon_json": {
        "watch": [
            "src"
        ],
        "ext": ".ts,.js",
        "ignore": [],
        "exec": "npx ts-node ./src/app.ts"
    },
    "rimraf": "npm i -D rimraf",
    "jest": "npm i -D jest types/jest ts-jest supertest",
    "load_jest_file_config": "npx jest --init",
    "jest_config_step_1": "no",
    "jest_config_step_2": "yes",
    "jest_config_step_3": "node",
    "jest_config_step_4": "yes",
    "jest_config_step_5": "V8",
    "jest_config_step_6": "no",
    "JWT": "npm install jsonwebtoken bcryptjs",
    "jsonwebtoken": "npm install -D @types/jsonwebtoken @types/bcryptjs"
}